{
  "name": "genericclickergame",
  "version": "1.0.0",
  "description": "Um jogo gen√©rico, mas divertido de clicker.",
  "main": "main.js",
  "author": "eupyetro022",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^28.3.3",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "follow-redirects": "^1.15.11"
  },
  "build": {
    "appId": "com.genericclickergame",
    "productName": "GenericClickerGame",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "forceCodeSigning": false,
    "win": {
      "target": "nsis",
      "icon": "icon.ico",
      "signAndEditExecutable": false,
      "verifyUpdateCodeSignature": false
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "GenericClickerGame"
    }
  }
}
